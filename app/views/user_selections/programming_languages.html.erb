<%= form_with url: user_programming_languages_path, method: :post do |form| %>
  <h2>Select Your Programming Languages</h2>
  <% @programming_languages.each do |language| %>
    <label>
      <%= form.check_box :programming_languages, { multiple: true }, language.name, nil %>
      <%= language.name %>
    </label>
    <br>
  <% end %>
  <%= form.submit 'Finish' %>
<% end %>
